        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <RadioButton
            android:id="@+id/q9a"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="a) all the forces are equally inclined"/>
        <RadioButton

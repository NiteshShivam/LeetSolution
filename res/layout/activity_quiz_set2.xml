        <RadioButton
            android:id="@+id/q9d"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="d) sum of resolved parts in the horizontal direction is zero (i.e. sumOf(H)= 0) "/>

    </RadioGroup>

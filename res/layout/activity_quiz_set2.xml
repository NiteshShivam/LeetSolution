        android:textStyle="bold"/>
    <RadioGroup
        android:id="@+id/rg15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <RadioButton
            android:id="@+id/q15a"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="a) meet at one point, but their lines of action do not lie on the same plane "/>
        <RadioButton
            android:id="@+id/q15b"
            android:layout_width="match_parent"
